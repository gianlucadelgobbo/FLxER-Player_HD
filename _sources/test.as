import flash.net.URLRequest;import flash.net.URLLoader;import flash.events.*;import com.adobe.serialization.json.JSONvar showProgress = function(progressevent:ProgressEvent):void {	trace(progressevent.bytesLoaded + ' / ' + progressevent.bytesTotal);}var decodeJSON = function(event:Event):void {	var loader:URLLoader = URLLoader(event.target) ;	var Json:Array = JSON.decode(loader.data) ;	trace(Json.length);	for (var key:Object in Json) {		trace(key);		//qui ti scorri l'oggetto json come se fosse xml	}}var _bootstrap = function():void {	var loader:URLLoader = new URLLoader() ;	var request:URLRequest = new URLRequest() ;	request.url = "http://www.demojuventusmember.com/_ajax.php" ;	request.method = URLRequestMethod.POST;	var variables:URLVariables = new URLVariables();	variables = {        name : 'return',        method : 'videoPlayerUserGet',        params : {            id: 1 // id dell'utente        }    };	request.data = variables;	loader.addEventListener(Event.COMPLETE, decodeJSON);	loader.addEventListener(ProgressEvent.PROGRESS, showProgress);	loader.load(request) ;}var login = function():void {	var loader:URLLoader = new URLLoader() ;	var request:URLRequest = new URLRequest() ;	request.url = "http://www.demojuventusmember.com/fake-login/id/1/q/jsdagjsdga/p/jsdkweho4nlds9h84" ;	loader.addEventListener(Event.COMPLETE, _bootstrap);	loader.load(request) ;}login();